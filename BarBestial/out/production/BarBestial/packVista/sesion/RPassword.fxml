<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="packVista.sesion.IU_RPass">
    <VBox prefHeight="330.0" prefWidth="297.0" style="-fx-background-color: #151a23;">
        <VBox alignment="CENTER" prefHeight="86.0" prefWidth="322.0">
            <Label text="¿HAS OLVIDADO LA" textFill="WHITE">
                <font>
                    <Font name="System Bold" size="24.0"/>
                </font>
            </Label>
            <Label text="CONTRASEÑA?" textFill="WHITE">
                <font>
                    <Font name="System Bold" size="24.0"/>
                </font>
            </Label>
        </VBox>
        <VBox alignment="CENTER" prefHeight="42.0" prefWidth="314.0">
            <Label text="Indica el correo del usuario" textFill="WHITE"/>
        </VBox>
        <VBox alignment="CENTER" prefHeight="130.0" prefWidth="302.0">
            <VBox maxWidth="-Infinity" prefHeight="16.0" prefWidth="213.0">
                <Label text="Correo" textFill="WHITE"/>
            </VBox>
            <TextField fx:id="txtCorreo" alignment="TOP_LEFT" maxWidth="-Infinity" prefHeight="32.0" prefWidth="213.0"/>
        </VBox>
        <VBox alignment="CENTER" prefHeight="55.0" prefWidth="297.0">
            <Button mnemonicParsing="false" onMouseClicked="#eventEnviarContrasena" prefHeight="32.0" prefWidth="76.0"
                    style="-fx-background-radius: 6px; -fx-background-color: #33cccc;" text="Enviar"/>
        </VBox>
        <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" nodeOrientation="LEFT_TO_RIGHT"
               onMouseClicked="#eventOpenLogin" prefHeight="23.0" prefWidth="286.0" text="Volver" textAlignment="RIGHT"
               textFill="#33cccc"/>
    </VBox>
</AnchorPane>
